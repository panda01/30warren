<div id="content-blocks">
    
    <%= f.fields_for :content_blocks, f.object.content_blocks.sort_by(&:position) do |content_block_field| %>
        <%= render 'admin/content_blocks/edit', f: f, content_block_field: content_block_field %>
    <% end %>

    <div class="content-block-wrapper base-block">
        <div id="cloneable-cb-options" class="new-content-block-options">
          <%= f.new_content_block_buttons %>
        </div>
    </div>
</div>